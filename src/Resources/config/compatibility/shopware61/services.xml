<?xml version="1.0" ?>
<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://symfony.com/schema/dic/services"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>

        <service id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Core\Content\Product\SalesChannel\Search\ProductSearchGateway">
            <argument type="service" id="sales_channel.product.repository" />
            <argument type="service" id="Shopware\Core\Content\Product\SearchKeyword\ProductSearchBuilderInterface" />
            <argument type="service" id="event_dispatcher" />
        </service>

        <service id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Core\Content\Product\SalesChannel\Listing\ProductListingGateway">
            <argument type="service" id="sales_channel.product.repository" />
            <argument type="service" id="event_dispatcher" />
        </service>

        <service id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Storefront\Page\Search\SearchPageLoader" public="true">
            <argument type="service" id="Shopware\Storefront\Page\GenericPageLoader" />
            <argument type="service"
                      id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Core\Content\Product\SalesChannel\Search\ProductSearchGateway" />
            <argument type="service" id="event_dispatcher" />
        </service>

        <service id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Core\Content\Product\SalesChannel\FindologicProductListingGateway">
            <argument type="service" id="sales_channel.product.repository" />
            <argument type="service" id="event_dispatcher" />
        </service>

        <service
                id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Core\Content\Product\Cms\ProductListingCmsElementResolver"
                decorates="Shopware\Core\Content\Product\Cms\ProductListingCmsElementResolver"
                public="true"
        >
            <argument type="service" id="FINDOLOGIC\FinSearch\CompatibilityLayer\Shopware61\Core\Content\Product\SalesChannel\FindologicProductListingGateway" />
            <tag name="shopware.cms.data_resolver" />
        </service>

    </services>
</container>
