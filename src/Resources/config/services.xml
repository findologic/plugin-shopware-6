<?xml version="1.0" ?>
<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://symfony.com/schema/dic/services"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>

        <service
            id="FINDOLOGIC\FinSearch\Core\Content\Product\SalesChannel\Listing\ProductListingFeaturesSubscriber"
            decorates="Shopware\Core\Content\Product\SalesChannel\Listing\ProductListingFeaturesSubscriber"
            public="true"
        >
            <tag name="kernel.event_subscriber" />
            <argument type="service" id="Doctrine\DBAL\Connection" />
            <argument type="service" id="property_group_option.repository" />
            <argument type="service"
                id="Shopware\Core\Content\Product\SalesChannel\Listing\ProductListingSortingRegistry" />
            <argument type="service" id="FINDOLOGIC\FinSearch\Findologic\Request\NavigationRequestFactory" />
            <argument type="service" id="FINDOLOGIC\FinSearch\Findologic\Request\SearchRequestFactory" />
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="FINDOLOGIC\FinSearch\Findologic\Resource\ServiceConfigResource" />
            <argument type="service" id="Shopware\Storefront\Page\GenericPageLoader" />
            <argument type="service" id="Symfony\Component\DependencyInjection\ContainerInterface" />
        </service>

        <service
            id="Shopware\Core\Content\Product\SearchKeyword\ProductSearchBuilderInterface"
            class="FINDOLOGIC\FinSearch\Core\Content\Product\SearchKeyword\ProductSearchBuilder"
        >
            <argument type="service" id="Shopware\Core\Content\Product\SearchKeyword\ProductSearchTermInterpreter" />
        </service>

        <service id="FINDOLOGIC\FinSearch\Core\Content\Product\SalesChannel\Search\ProductSearchGateway">
            <argument type="service" id="sales_channel.product.repository" />
            <argument type="service" id="Shopware\Core\Content\Product\SearchKeyword\ProductSearchBuilderInterface" />
            <argument type="service" id="event_dispatcher" />
        </service>

        <service id="FINDOLOGIC\FinSearch\Storefront\Page\Search\SearchPageLoader" public="true">
            <argument type="service" id="Shopware\Storefront\Page\GenericPageLoader" />
            <argument type="service"
                id="FINDOLOGIC\FinSearch\Core\Content\Product\SalesChannel\Search\ProductSearchGateway" />
            <argument type="service" id="event_dispatcher" />
        </service>

        <service
            id="FINDOLOGIC\FinSearch\Storefront\Controller\SearchController"
            decorates="Shopware\Storefront\Controller\SearchController"
            public="true"
        >
            <argument type="service" id="FINDOLOGIC\FinSearch\Storefront\Page\Search\SearchPageLoader" />
            <argument type="service" id="Shopware\Storefront\Page\Suggest\SuggestPageLoader" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>

        <service
            id="FINDOLOGIC\FinSearch\Subscriber\FrontendSubscriber"
            class="FINDOLOGIC\FinSearch\Subscriber\FrontendSubscriber"
            public="true"
        >
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="FINDOLOGIC\FinSearch\Findologic\Resource\ServiceConfigResource" />
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="FINDOLOGIC\FinSearch\Controller\ExportController" public="true">
            <argument type="service" id="logger" />
            <tag name="kernel.event_subscriber" />
            <tag name="monolog.logger" channel="file" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
            <argument type="service" id="Shopware\Storefront\Framework\Routing\Router" />
            <argument type="service" id="FINDOLOGIC\FinSearch\Export\HeaderHandler" />
        </service>

        <service
            class="FINDOLOGIC\FinSearch\Export\FindologicProductFactory"
            id="FINDOLOGIC\FinSearch\Export\FindologicProductFactory"
            public="true"
        />

        <service
            class="FINDOLOGIC\FinSearch\Findologic\Client\ServiceConfigClientFactory"
            id="FINDOLOGIC\FinSearch\Findologic\Client\ServiceConfigClientFactory"
            public="true"
        />

        <service
            class="FINDOLOGIC\FinSearch\Findologic\Resource\ServiceConfigResource"
            id="FINDOLOGIC\FinSearch\Findologic\Resource\ServiceConfigResource"
            public="true"
        >
            <argument id="serializer.mapping.cache.symfony" type="service" />
            <argument id="FINDOLOGIC\FinSearch\Findologic\Client\ServiceConfigClientFactory" type="service" />
        </service>

        <service
            class="FINDOLOGIC\FinSearch\Findologic\Request\SearchRequestFactory"
            id="FINDOLOGIC\FinSearch\Findologic\Request\SearchRequestFactory"
            public="true"
        >
            <argument id="serializer.mapping.cache.symfony" type="service" />
            <argument id="service_container" type="service" />
        </service>

        <service
            class="FINDOLOGIC\FinSearch\Findologic\Request\NavigationRequestFactory"
            id="FINDOLOGIC\FinSearch\Findologic\Request\NavigationRequestFactory"
            public="true"
        >
            <argument id="serializer.mapping.cache.symfony" type="service" />
            <argument id="service_container" type="service" />
        </service>

        <service id="FINDOLOGIC\FinSearch\Resources\app\storefront\src\snippets\en_GB\SnippetFile_en_GB" public="true">
            <tag name="shopware.snippet.file" />
        </service>

        <service id="FINDOLOGIC\FinSearch\Resources\app\storefront\src\snippets\de_DE\SnippetFile_de_DE" public="true">
            <tag name="shopware.snippet.file" />
        </service>

        <service
            id="FINDOLOGIC\FinSearch\Utils\TwigExtension"
            class="FINDOLOGIC\FinSearch\Utils\TwigExtension"
            public="false"
        >
            <tag name="twig.extension" />
        </service>

        <service id="FINDOLOGIC\FinSearch\Export\HeaderHandler" public="true">
            <argument id="service_container" type="service"/>
        </service>

    </services>
</container>
